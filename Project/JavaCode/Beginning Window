import javax.swing.*;    
public class First{    
First(){   

// Frame is Window
JFrame f=new JFrame();    
  
// Giant button that says click
JButton b=new JButton("click");    
b.setBounds(130,100,100, 40);            
f.add(b);    

// frame size, layout, visibility
f.setSize(300,400);    
f.setLayout(null);    
f.setVisible(true);    
            
// set close            
f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);    
}    

public static void main(String[] args) {    
    new First();    
}    
}    
